{
  "name": "solar-system",
  "version": "1.0.0",
  "description": "Solar System 3D Visualization",
  "scripts": {
    "start:api": "cd api && npm run dev",
    "start:app": "cd app && npm run dev",
    "dev": "concurrently \"npm run start:api\" \"npm run start:app\"",
    "install:all": "cd api && npm install && cd ../app && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
